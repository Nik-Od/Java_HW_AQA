<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TestSuite">
    <test name="PositiveTests">
        <groups>
            <run>
                <include name="positive"/>
            </run>
        </groups>
    </test>

    <test name="NegativeTests">
        <groups>
            <run>
                <include name="negative"/>
            </run>
        </groups>
    </test>
</suite>
